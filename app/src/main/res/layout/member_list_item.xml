<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/list_item_background_activated"
    style="@style/default_linear_layout_padding"
    >

    <TextView
        android:id="@+id/list_item_name"
        android:text="Member Name"
        style="@style/two_row_item_title"
        />

    <TextView
        android:id="@+id/list_item_id"
        android:text="Member ID"
        style="@style/two_row_item_secondary"
        />

    <TextView
        android:id="@+id/list_item_key"
        android:text="Member Key"
        style="@style/two_row_item_secondary"
        />

    <TextView
        android:id="@+id/list_item_schedule_list"
        android:text="Schedule List"
        style="@style/two_row_item_secondary"
        />

</LinearLayout>
